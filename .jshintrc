{
  "node": true,
  "onecase": true,
  "maxlen": 140,
  "boss": true,
  "latedef": true,
  "noarg": true,
  "eqeqeq": true,
  "eqnull": true,
  "undef": true,
  "strict": true,
  "trailing": true,
  "expr": true,
  "overrides": {
    "test/**/*.js": {
      "predef": [
        "expect",
        "describe",
        "it",
        "before",
        "beforeEach",
        "after",
        "afterEach"
      ]
    }
  }
}
